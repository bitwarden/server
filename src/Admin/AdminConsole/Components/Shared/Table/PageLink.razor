@if (Page.HasValue)
{
    <li class="page-item">
        <button class="page-link" name="@(FormKey!)" value="@Page">
            @Label
        </button>
    </li>
}
else
{
    <li class="page-item disabled">
        <a class="page-link" href="#" tabindex="-1">
            @Label
        </a>
    </li>
}
